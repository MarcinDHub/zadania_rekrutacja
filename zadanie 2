def func(number_list, n):
    missing_elements = []

    # checking if element from range 1-n is in our list
    # if not: add this element to result array
    for i in range(1, n+1):
        if i in number_list:
            pass
        else:
            missing_elements.append(i)

    print(missing_elements)


def main():
	number_list = [2, 3, 7, 4, 9]
	n = 10

	func(number_list, n)


if __name__ == '__main__':
    main()
